{
  "bookingFilters": [
    {
      "description": "valid firstname filter",
      "query": { "firstname": "Jim" },
      "expectAtLeastOne": true
    },
    {
      "description": "valid lastname filter",
      "query": { "lastname": "Brown" },
      "expectAtLeastOne": true
    },
    {
      "description": "valid checkin filter",
      "query": { "checkin": "2025-06-10" },
      "expectAtLeastOne": true
    },
    {
      "description": "valid checkout filter",
      "query": { "checkout": "2025-06-15" },
      "expectAtLeastOne": true
    },
    {
      "description": "invalid firstname filter",
      "query": { "firstname": "NonExistentName" },
      "expectAtLeastOne": false
    },
    {
      "description": "invalid lastname filter",
      "query": { "lastname": "NonExistentLast" },
      "expectAtLeastOne": false
    },
    {
      "description": "invalid checkin filter",
      "query": { "checkin": "2099-01-01" },
      "expectAtLeastOne": false
    },
    {
      "description": "invalid checkout filter",
      "query": { "checkout": "2099-12-31" },
      "expectAtLeastOne": false
    }
  ]
}
